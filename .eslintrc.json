{
    "root": true,
    "extends": ["@freecodecamp/eslint-config", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "max-len": [
            "error",
            { "code": 80, "ignoreUrls": true, "ignoreTemplateLiterals": true }
        ],
        "prettier/prettier": "error"
    }
}
